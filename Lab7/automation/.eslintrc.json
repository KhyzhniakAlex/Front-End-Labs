{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "modules": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "indent": [
      "error",
      4
    ],
    "linebreak-style": 0,
    "global-require": 0,
    "eslint linebreak-style": [0, "error", "windows"],
    "import/no-extraneous-dependencies": "off",
    "no-use-before-define": "off",
    "no-return-assign": "off",
    "no-param-reassign": "off",
    "no-underscore-dangle": "off",
    "max-len": [
      "warn",
      {
        "code": 170
      }
    ],
    "no-const-assign": "warn",
    "no-this-before-super": "warn",
    "no-undef": "error",
    "no-unreachable": "warn",
    "no-unused-vars": "warn",
    "semi": "warn",
    "constructor-super": "warn",
    "valid-typeof": "warn",
    "no-console": "off",
    "space-before-blocks": "warn",
    "no-trailing-spaces": "warn",
    "space-in-parens": "warn",
    "arrow-spacing": "warn",
    "semi-spacing": [
      "warn",
      {
        "before": false,
        "after": true
      }
    ],
    "block-spacing": "warn",
    "no-extra-semi": "warn",
    "no-multi-spaces": "warn",
    "one-var": [
      "warn",
      "never"
    ],
    "prefer-destructuring": [
      "error",
      {
        "object": false,
        "array": false
      }
    ],
    "object-curly-spacing": "off",
    "class-methods-use-this": 0,
    "no-restricted-syntax": "warn",
    "no-await-in-loop": "off",
    "radix": "off"
  },
  "globals": {
    "afterEach": true,
    "beforeEach": true,
    "beforeAll": true,
    "afterAll": true,
    "jasmine": true,
    "describe": true,
    "it": true,
    "moment": true,
    "_": true,
    "expect": true,
    "xit": true,
    "xdescribe": true,
    "logger": true,
    "element": true,
    "by": true,
    "protractor": true,
    "browser": true,
    "utils": true,
    "$": true,
    "$$": true,
    "container": true,
    "tempFolder": true
  },
  "extends": "airbnb-base"
}
